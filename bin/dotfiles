#!/bin/sh
#
# dotfiles
#
# `dotfiles` handles installation, updates, things like that. Run it periodically
# to make sure you're on the latest and greatest.
# Modified from https://github.com/holman/dotfiles/blob/master/bin/dot

export ZSH=${ZSH:=$HOME/.dotfiles}

# Install homebrew packages
$ZSH/script/bootstrap && $ZSH/script/install
